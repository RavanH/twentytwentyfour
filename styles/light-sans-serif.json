{
	"version": 2,
	"title": "Light Sans Serif",
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"styles": {
		"blocks": {
			"core/paragraph": {
				"css": "&.is-style-heading {font-family: var(--wp--preset--font-family--system-font)}"
			},
			"core/pullquote": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-font)"
				}
			},
			"core/quote": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-font)"
				}
			}
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-font)"
				}
			}
		}
	}
}